{
  "name": "svg-icons",
  "version": "0.7.0",
  "description": "Snippets for popular SVG icons, including Octicons, Evil Icons, Open Iconic, SmartIcons Glyphs, and Bytesize",
  "author": {
    "name": "Jan T. Sott",
    "url": "http://github.com/idleberg"
  },
  "keywords": [
    "svg icons",
    "octicons",
    "evil icons",
    "glyphs"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/idleberg/atom-svg-icons"
  },
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "devDependencies": {
    "eslint": "^7.32.0",
    "eslint-plugin-json": "^3.1.0",
    "husky": "^7.0.0",
    "lint-staged": "^11.1.2"
  },
  "scripts": {
    "lint": "eslint --ignore-path .gitignore --no-error-on-unmatched-pattern ./snippets/*.json",
    "prepare": "husky install",
    "test": "npm run lint"
  },
  "lint-staged": {
    "*.json": "eslint --cache --fix"
  },
  "volta": {
    "node": "14.17.6",
    "yarn": "1.22.11",
    "npm": "6.14.15"
  }
}
